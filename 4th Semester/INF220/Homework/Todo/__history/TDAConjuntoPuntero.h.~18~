struct Nodo
{
	int dato;
    Nodo *sig;
};

class TDAConjuntoPuntero
{
	private:
		int cant;
		Nodo *PtrConj;
	public:
		void crear();
        void inserta(int e);
		void suprime(int e);
		void imprime();

		int cardinal();
		int ordinal(int e);

		bool vacio();
		bool pertenece(int e);

};
