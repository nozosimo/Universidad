#ifndef TDAConjuntoVectorH
#define TDAConjuntoVectorH
#endif
#include <iostream>
#include <ctime>
#include <cstdlib>



static const int maxi  = 1000000;

class TDAConjuntoVector
{
	private:
		bool v[maxi+1];
		int cant;
	public:
		void crear();
        void inserta(int e);
		void suprime(int e);
		// Funcion anadida
		void imprime();

        int cardinal();
        int ordinal(int elemento);

		bool vacio();
        bool pertenece(int e);

		int muestrea();


};
