#include <ctime>
#include <cstdlib>



static const int maxi  = 100;

class TDAConjuntoVector
{
	private:
		bool v[maxi+1];
		int cant;
	public:
        void crear();
		bool vacio();
		int cardinal();
        int ordinal(int elemento);
        bool pertenece(int e);
		void inserta(int e);
		void suprime(int e);
        int muestrea();
};
