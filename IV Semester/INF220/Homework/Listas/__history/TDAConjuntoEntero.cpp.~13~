{
    elem = 0;
}

bool TDAConjuntoEntero::vacio()
{
    return(elem==0);
}

int TDAConjuntoEntero::cardinal()
{
	if(!vacio())
	{
		return int(log10(double(elem))
		+1);
    }

}

