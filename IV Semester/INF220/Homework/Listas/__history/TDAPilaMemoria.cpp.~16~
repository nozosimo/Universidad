//---------------------------------------------------------------------------

#pragma hdrstop

#include "TDAPilaMemoria.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

void TDAPilaMemoria::crear()
{
    Tope = 0;
}

bool TDAPilaMemoria::vacia()
{
    return (Tope==0);
}

int TDAPilaMemoria::cima()
{
	if(vacia())
	{
        throw("Error...");
	}else
	{
        return mem.obtener_dato(Tope,1);
	}


}

