#include <vector>

static const int Max = 100;

class TDAListaVector
{
	public:

        std::vector<int> Elementos;
		int Longitud;

		void crear();
		int fin();
		int primero();
		int siguiente(int direccion);
		int anterior(int direccion);
		bool vacia();
        int recupera(int direccion);

		int buscar(TDAListaVector L,int elemento);
};
